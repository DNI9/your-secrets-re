{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "run-s -l clean build:css build:remix",
    "build:remix": "remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "run-p -l dev:css dev:remix",
    "dev:remix": "node -r dotenv/config node_modules/.bin/remix dev",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "postinstall": "remix setup node",
    "clean": "rimraf .cache api/_build public/build",
    "start": "node -r dotenv/config node_modules/.bin/remix-serve api/_build",
    "lint": "eslint ./app"
  },
  "dependencies": {
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@remix-run/vercel": "^1.1.3",
    "@supabase/supabase-js": "^1.30.0",
    "dotenv": "^16.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.28.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.19",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
